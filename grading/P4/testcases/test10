tid=`basename $0`
mount=mount/$tid

mkdir -p $mount
. $(dirname $0)/mount.sh $mount; mountcode=$?

if [ "$mountcode" -ne 0 ]; then
        exit $mountcode
fi

file=$mount/$tid
returncode=1
touch $file; mv $file $file.mv
if [ -f "$file.mv" ]; then
	returncode=0
fi

fusermount -u $mount
exit $returncode
